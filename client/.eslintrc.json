{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "linebreak-style": 0,
    "require-jsdoc": 0,
    "indent": [
      "error",
      2
    ]
  }
}
